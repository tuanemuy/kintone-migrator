{
  "name": "kintone-migrator",
  "version": "0.1.0",
  "description": "kintone form schema migration tool",
  "license": "MIT",
  "author": "Hikaru Otabe",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuanemuy/kintone-migrator.git"
  },
  "keywords": [
    "kintone",
    "migration",
    "schema",
    "cli",
    "form"
  ],
  "type": "module",
  "bin": {
    "kintone-migrator": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "dotenv -- tsx src/cli/index.ts",
    "dev:diff": "dotenv -- tsx src/cli/index.ts diff",
    "dev:migrate": "dotenv -- tsx src/cli/index.ts migrate",
    "dev:override": "dotenv -- tsx src/cli/index.ts override",
    "dev:capture": "dotenv -- tsx src/cli/index.ts capture",
    "dev:dump": "dotenv -- tsx src/cli/index.ts dump",
    "build": "tsdown",
    "start": "dotenv -- node dist/index.js",
    "prepublishOnly": "pnpm build",
    "lint": "biome lint",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "typecheck": "tsgo",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "@kintone/rest-api-client": "^6.1.1",
    "gunshi": "^0.28.0",
    "picocolors": "^1.1.1",
    "valibot": "^1.2.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^25.2.2",
    "@typescript/native-preview": "7.0.0-dev.20260209.1",
    "@vitest/coverage-v8": "^4.0.18",
    "dotenv-cli": "^11.0.0",
    "tsdown": "^0.12.4",
    "tsx": "^4.19.4",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@^7.3.1",
    "vitest": "^4.0.18"
  }
}
